{
	"name": "超图imooc快进",
	"version": "1.0",
	"manifest_version": 2,
	"description": "用于快速完成网上公选课，仅作学习交流使用",
	"icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage"
  ],
  "options_page": "options.html"
}
